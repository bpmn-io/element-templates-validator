[
  {
    "name": "Reusable Script Template",
    "id": "io.camunda.examples.Script",
    "description": "A reusable script template",
    "version": 1,
    "engines": {
      "camunda": "^8.6"
    },
    "appliesTo": [
      "bpmn:Task"
    ],
    "elementType": {
      "value": "bpmn:ScriptTask"
    },
    "properties": [
      {
        "type": "Hidden",
        "value": "=1 + 1",
        "binding": {
          "type": "zeebe:script",
          "property": "expression"
        }
      },
      {
        "type": "Hidden",
        "value": "aResultVariable",
        "binding": {
          "type": "zeebe:script",
          "property": "resultVariable"
        }
      }
    ]
  }
]